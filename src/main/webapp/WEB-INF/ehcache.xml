<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd" updateCheck="false">

       <defaultCache
               eternal="false"
               maxElementsInMemory="1000"
               overflowToDisk="false"
               diskPersistent="false"
               timeToIdleSeconds="0"
               timeToLiveSeconds="600"
               memoryStoreEvictionPolicy="LRU"/>

       <cache
               name="departmentCache"
               eternal="false"
               maxElementsInMemory="100"
               overflowToDisk="false"
               diskPersistent="false"
               timeToIdleSeconds="0"
               timeToLiveSeconds="86400"
               memoryStoreEvictionPolicy="LRU" />

        <cache
                name="siteCache"
                eternal="false"
                maxElementsInMemory="1000"
                overflowToDisk="false"
                diskPersistent="false"
                timeToIdleSeconds="0"
                timeToLiveSeconds="3600"
                memoryStoreEvictionPolicy="LRU" />

        <cache
                name="kpuCache"
                eternal="false"
                maxElementsInMemory="1000"
                overflowToDisk="false"
                diskPersistent="false"
                timeToIdleSeconds="0"
                timeToLiveSeconds="3600"
                memoryStoreEvictionPolicy="LRU" />

        <cache
                name="latestCache"
                eternal="false"
                maxElementsInMemory="1000"
                overflowToDisk="false"
                diskPersistent="false"
                timeToIdleSeconds="0"
                timeToLiveSeconds="60"
                memoryStoreEvictionPolicy="LRU" />

        <cache
                name="hotCache"
                eternal="false"
                maxElementsInMemory="1000"
                overflowToDisk="false"
                diskPersistent="false"
                timeToIdleSeconds="600"
                timeToLiveSeconds="3600"
                memoryStoreEvictionPolicy="LRU" />

        <cache
                name="tendencyCache"
                eternal="false"
                maxElementsInMemory="1000"
                overflowToDisk="false"
                diskPersistent="false"
                timeToIdleSeconds="0"
                timeToLiveSeconds="3600"
                memoryStoreEvictionPolicy="LRU" />

        <cache
                name="semesterCache"
                eternal="false"
                maxElementsInMemory="1000"
                overflowToDisk="false"
                diskPersistent="false"
                timeToIdleSeconds="0"
                timeToLiveSeconds="3600"
                memoryStoreEvictionPolicy="LRU" />

        <cache
                name="lectureCache"
                eternal="false"
                maxElementsInMemory="1000"
                overflowToDisk="false"
                diskPersistent="false"
                timeToIdleSeconds="0"
                timeToLiveSeconds="3600"
                memoryStoreEvictionPolicy="LRU" />


</ehcache>